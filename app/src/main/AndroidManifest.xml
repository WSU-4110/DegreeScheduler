<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.DegreeSchedulerApp.degreescheduler">

    <application
        android:allowBackup="true"
        android:icon="@drawable/view_download"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DegreeScheduler">
        <activity android:name=".Reset"></activity>
        <activity android:name=".PasswordReset" />
        <activity android:name=".Register" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".webview"/>
        <activity android:name=".CreateSchedule" android:exported="true"/>
        <activity android:name=".LogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>